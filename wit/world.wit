package betty-blocks:custom@0.1.0;

interface actions {
   type json-string = string;

   record payload {
      input: json-string,
   }

   record input {
      action-id: string,
      payload: payload,
   }

   record output {
      %result: json-string,
   }

   call: func(input: input) -> result<output, string>;
}

world main {

   import betty-blocks:http-request/http;
   import betty-blocks:http-request-part-two/http;

   export actions;
}
